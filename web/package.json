{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && build:TWD && vite build",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "build:TWD": "tailwindcss -i src/input.css -o ./src/generated/output.css",
    "tailwind:w": "tailwindcss -i node_modules/tailwindcss/tailwind.css -o src/generated/output.css --watch",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "DEBUG=eslint:cli-engine npx eslint . --ext .ts,.tsx --fix",
    "lint:style": "npx stylelint src/**/*.scss"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.81",
    "@mui/material": "^5.7.0",
    "@tinymce/tinymce-react": "^4.0.0",
    "@types/react-swipeable-views": "^0.13.1",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "i18next": "^21.8.0",
    "i18next-browser-languagedetector": "^6.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^11.16.9",
    "react-loading-skeleton": "^3.1.0",
    "react-router-dom": "^6.3.0",
    "react-swipeable-views": "^0.14.0",
    "tinymce": "^6.0.2",
    "tinymce-i18n": "^22.5.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.32",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.16",
    "@types/react-html-parser": "^2.0.2",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwind": "^2.2.12",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "^2.9.8"
